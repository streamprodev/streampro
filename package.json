    "firebase": "^10.0.0",
    "fs": "^0.0.1-security",
    "iconsax-react": "^0.0.8",
    "minisearch": "^6.1.0",
    "node-machine-id": "^1.1.12",
    "react": "^18.2.0",
    "react-autocomplete-hint": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "react-input-mask": "^2.0.4",
    "react-otp-input": "^3.0.2",
